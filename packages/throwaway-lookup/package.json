  "author": "Iocium",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "auto-changelog": "^2.5.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.8.3"
  }
