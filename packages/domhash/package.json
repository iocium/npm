{
  "name": "@iocium/domhash",
  "sideEffects": false,
  "version": "0.2.0",
  "description": "Structure- and layout-aware perceptual hashing for HTML/DOM trees.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/umd/index.umd.js",
  "unpkg": "dist/umd/index.umd.js",
  "jsdelivr": "dist/umd/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "profile": "node scripts/profile.js",
    "benchmark": "node scripts/benchmark.js",
    "lint": "eslint src --ext .ts"
  },
  "bin": {
    "domhash": "dist/cli.js"
  },
  "keywords": [
    "hash",
    "dom",
    "layout",
    "html",
    "similarity",
    "perceptual"
  ],
  "author": "iocium",
  "license": "MIT",
  "dependencies": {
    "@noble/hashes": "^1.8.0",
    "commander": "^14.0.0",
    "diff": "^8.0.2",
    "htmlparser2": "^10.0.0",
    "linkedom": "^0.18.11",
    "puppeteer-core": "^24.11.2",
    "typedoc": "^0.28.15"
  },
  "devDependencies": {
    "@types/bun": "^1.2.17",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "auto-changelog": "^2.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.2",
    "jsdom": "^27.2.0",
    "miniflare": "^4.20250617.4",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
